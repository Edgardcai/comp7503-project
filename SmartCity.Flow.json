    {
        "id": "function_prepare_chart_aqi",
        "type": "function",
        "z": "smart_city_tab",
        "name": "准备图表数据",
        "func": "// 为Dashboard准备数据\nvar chartData = [];\nvar gaugeData = 0;\n\nif (Array.isArray(msg.payload)) {\n    // 计算平均AQI\n    var totalAqi = 0;\n    msg.payload.forEach(function(record) {\n        totalAqi += record.aqi;\n        chartData.push({\n            x: record.station,\n            y: record.aqi\n        });\n    });\n    gaugeData = Math.round(totalAqi / msg.payload.length);\n}\n\n// 分别发送到不同的Dashboard节点\nreturn [\n    {payload: chartData, topic: \"AQI by Station\"},\n    {payload: gaugeData, topic: \"Average AQI\"}\n];",
        "outputs": 2,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 950,
